{
  "name": "laravel/rankings",
  "description": "User ranking package for Laravel based on DDD principles",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\Rankings\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "App\\Rankings\\LaravelRankingsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
